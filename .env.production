# App Environment
VITE_APP_ENV=production
VITE_APP_TITLE=ToAgro - Аграрний маркетплейс
VITE_APP_DESCRIPTION=Маркетплейс для купівлі і продажу сільськогосподарської продукції та обладнання

# API Configuration
VITE_API_URL=https://api.toagro.com
VITE_WS_URL=wss://ws.toagro.com

# Authentication
VITE_JWT_SECRET_KEY=production-secret-key-here
VITE_TOKEN_EXPIRY=7d

# Payment Gateways
VITE_STRIPE_PUBLIC_KEY=pk_live_your_stripe_public_key
VITE_PAYPAL_CLIENT_ID=your_live_paypal_client_id

# Third-party Services
VITE_GOOGLE_MAPS_API_KEY=your_google_maps_api_key
VITE_SENTRY_DSN=https://production-sentry-dsn@sentry.io
VITE_ANALYTICS_ID=G-PRODUCTION-ANALYTICS-ID

# Upload Settings
VITE_MAX_FILE_SIZE=5242880 # 5MB in bytes
VITE_ALLOWED_IMAGE_TYPES=image/jpeg,image/png,image/webp

# Feature Flags
VITE_ENABLE_NOTIFICATIONS=true
VITE_ENABLE_CHAT=true
VITE_ENABLE_PAYMENTS=true
VITE_ENABLE_ADMIN_PANEL=true

# API Rate Limiting
VITE_API_RATE_LIMIT=1000
VITE_API_RATE_WINDOW=60000 # 1 minute in milliseconds

# Debug Settings
VITE_DEBUG=false
VITE_LOG_LEVEL=error

# Contact Information
VITE_SUPPORT_EMAIL=support@toagro.com
VITE_SUPPORT_PHONE=+380123456789
VITE_SUPPORT_ADDRESS="вул. Хрещатик, 1, Київ, Україна"

# Social Media Links
VITE_FACEBOOK_URL=https://facebook.com/toagro
VITE_TWITTER_URL=https://twitter.com/toagro
VITE_INSTAGRAM_URL=https://instagram.com/toagro
VITE_TELEGRAM_URL=https://t.me/toagro

# Version Control
VITE_APP_VERSION=1.0.0
VITE_BUILD_DATE=${BUILD_DATE}